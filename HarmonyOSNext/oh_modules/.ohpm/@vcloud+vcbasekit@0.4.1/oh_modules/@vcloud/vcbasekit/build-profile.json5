{
  "apiType": "stageMode",
  "buildOption": {
    "externalNativeOptions": {
      "path": "../../CMakeLists.txt",
      "abiFilters": [
        "arm64-v8a"
      ],
      "cppFlags": ""
    },
    "arkOptions": {
      // "apPath": "./modules.ap"  /* Profile used for profile-guided optimization (PGO), a compiler optimization technique to improve app runtime performance. */
    },
    "nativeLib": {
      "debugSymbol": {
        "strip": true,
      },
      "filter": {
        "excludes": [
          "**/libc++_shared.so",
        ],
      },
      "headerPath": "../../lib/include/vcbasekit"
    }
  },
  "buildOptionSet": [
    {
      "name": "release",
      "externalNativeOptions": {
        "arguments": "-DCMAKE_BUILD_TYPE=RelWithDebInfo"
      },
      "arkOptions": {
        "obfuscation": {
          "ruleOptions": {
            "enable": false,
            "files": [
              "./obfuscation-rules.txt"
            ]
          },
          "consumerFiles": [
            "./consumer-rules.txt"
          ]
        }
      },
    },
    {
      "name": "debug",
      "externalNativeOptions": {
        "arguments": "-DCMAKE_BUILD_TYPE=Debug"
      }
    }
  ],
  "targets": [
    {
      "name": "default"
    }
  ]
}